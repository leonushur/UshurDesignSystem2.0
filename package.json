{
  "name": "@ushur/design-system",
  "version": "0.1.0",
  "type": "module",
  "description": "Ushur Design System - A comprehensive React component library built with TypeScript, Tailwind CSS 4, and React Aria Components",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./base": {
      "import": {
        "types": "./dist/components/base/index.d.ts",
        "default": "./dist/base.js"
      },
      "require": {
        "types": "./dist/components/base/index.d.ts",
        "default": "./dist/base.cjs"
      }
    },
    "./application": {
      "import": {
        "types": "./dist/components/application/index.d.ts",
        "default": "./dist/application.js"
      },
      "require": {
        "types": "./dist/components/application/index.d.ts",
        "default": "./dist/application.cjs"
      }
    },
    "./primitives": {
      "import": {
        "types": "./dist/components/primitives/index.d.ts",
        "default": "./dist/primitives.js"
      },
      "require": {
        "types": "./dist/components/primitives/index.d.ts",
        "default": "./dist/primitives.cjs"
      }
    },
    "./foundations": {
      "import": {
        "types": "./dist/components/foundations/index.d.ts",
        "default": "./dist/foundations.js"
      },
      "require": {
        "types": "./dist/components/foundations/index.d.ts",
        "default": "./dist/foundations.cjs"
      }
    },
    "./marketing": {
      "import": {
        "types": "./dist/components/marketing/index.d.ts",
        "default": "./dist/marketing.js"
      },
      "require": {
        "types": "./dist/components/marketing/index.d.ts",
        "default": "./dist/marketing.cjs"
      }
    },
    "./shared-assets": {
      "import": {
        "types": "./dist/components/shared-assets/index.d.ts",
        "default": "./dist/shared-assets.js"
      },
      "require": {
        "types": "./dist/components/shared-assets/index.d.ts",
        "default": "./dist/shared-assets.cjs"
      }
    },
    "./hooks": {
      "import": {
        "types": "./dist/hooks/index.d.ts",
        "default": "./dist/hooks.js"
      },
      "require": {
        "types": "./dist/hooks/index.d.ts",
        "default": "./dist/hooks.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/utils/index.d.ts",
        "default": "./dist/utils.js"
      },
      "require": {
        "types": "./dist/utils/index.d.ts",
        "default": "./dist/utils.cjs"
      }
    },
    "./styles": "./dist/design-system.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ushur/design-system.git"
  },
  "author": "Ushur",
  "license": "MIT",
  "keywords": [
    "react",
    "design-system",
    "component-library",
    "ui",
    "typescript",
    "tailwindcss",
    "react-aria",
    "accessible",
    "ushur"
  ],
  "typesVersions": {
    "*": {
      "base": ["./dist/components/base/index.d.ts"],
      "application": ["./dist/components/application/index.d.ts"],
      "primitives": ["./dist/components/primitives/index.d.ts"],
      "foundations": ["./dist/components/foundations/index.d.ts"],
      "marketing": ["./dist/components/marketing/index.d.ts"],
      "shared-assets": ["./dist/components/shared-assets/index.d.ts"],
      "hooks": ["./dist/hooks/index.d.ts"],
      "utils": ["./dist/utils/index.d.ts"]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.build.json",
    "build:lib": "vite build && tsc -p tsconfig.build.json",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:storybook": "vitest --project=storybook"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@tiptap/extension-highlight": "^3.13.0",
    "@tiptap/extension-image": "^3.13.0",
    "@tiptap/extension-link": "^3.13.0",
    "@tiptap/extension-placeholder": "^3.13.0",
    "@tiptap/extension-text-align": "^3.13.0",
    "@tiptap/extension-typography": "^3.13.0",
    "@tiptap/extension-underline": "^3.13.0",
    "@tiptap/pm": "^3.13.0",
    "@tiptap/react": "^3.13.0",
    "@tiptap/starter-kit": "^3.13.0",
    "@untitledui-pro/icons": "^0.0.2",
    "@untitledui/file-icons": "^0.0.8",
    "@untitledui/icons": "^0.0.19",
    "motion": "^12.23.12",
    "react-aria": "^3.42.0",
    "react-aria-components": "^1.13.0",
    "react-hotkeys-hook": "^5.1.0",
    "react-is": "^19.1.0",
    "react-router": "^7.8.0",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-react-aria-components": "^2.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@tailwindcss/vite": "^4.1.11",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "@mdx-js/react": "^3.1.1",
    "@storybook/addon-a11y": "^10.0.8",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-vitest": "^10.0.8",
    "@storybook/mdx2-csf": "^1.1.0",
    "@storybook/react-vite": "^10.0.8",
    "@tailwindcss/postcss": "^4.1.11",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "@vitest/browser-playwright": "^4.0.10",
    "@vitest/coverage-v8": "^4.0.10",
    "fontmin": "^1.1.1",
    "globals": "^16.3.0",
    "otf2ttf": "^1.1.2",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "storybook": "^10.0.8",
    "ttf2woff": "^3.0.0",
    "ttf2woff2": "^8.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.1",
    "vitest": "^4.0.10"
  }
}
